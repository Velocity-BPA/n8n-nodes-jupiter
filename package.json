  "description": "A comprehensive n8n community node for Jupiter DEX aggregator on Solana providing 21 resources and 200+ operations for swaps, limit orders, DCA, perpetuals, JLP, governance, and more.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-jupiter.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-jupiter#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-jupiter/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "jupiter",
    "solana",
    "dex",
    "swap",
    "defi",
    "blockchain",
    "crypto",
    "trading",
    "limit-order",
    "dca",
    "perpetuals",
    "jlp",
    "jup"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "COMMERCIAL_LICENSE.md",
    "LICENSING_FAQ.md"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/JupiterNetwork.credentials.js",
      "dist/credentials/JupiterApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Jupiter/Jupiter.node.js",
      "dist/nodes/Jupiter/JupiterTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
